{
	"compilerOptions": {
		"allowJs": true,
		"esModuleInterop": true,
		"lib": ["ES2015", "ES2017"],
		"moduleResolution": "Node",
		"skipLibCheck": true,
		"strict": true,
		"noFallthroughCasesInSwitch": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"target": "ESNext"
	}
}
